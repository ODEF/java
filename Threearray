class Threearray{

  public static void main(String[] args){

  int[][] num = new int[3][3];
  int q = 0; 
  for(var i=0; i<3; i++){
    for(var j=0; j<3; j++){
      num[i][j]=q;
      ++q;
    }
  }

  for(var i=0; i<3; i++){
    for(var j=0; j<3; j++){
      System.out.print(num[i][j] + " ");
    }
    System.out.println(" ");
  }
 
 
 System.out.println(" ");

  }
}
